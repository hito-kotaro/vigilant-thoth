generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model mst_books {
  id Int @id @default(autoincrement())
  isbn String @unique
  title String
  author String
  publisher String
  published_at String
  created_at DateTime
  updated_at DateTime?
  deleted_at DateTime?
}
